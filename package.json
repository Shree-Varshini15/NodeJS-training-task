{
  "name": "simplerPams",
  "version": "0.0.1",
  "description": "Awesome project developed with TypeORM.",
  "scripts": {
    "watch": "tsc -w",
    "dev": "nodemon build/index.js",
    "start:dev": "concurrently \"tsc -w\" \"nodemon build/index.js\"",
    "build": "tsc",
    "start": "ts-node src/index.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "tsx:cli": "tsx ./node_modules/typeorm/cli.js -d ./src/data-source.ts",
    "tsx:cli:migration:generate": "npm run tsx:cli migration:generate",
    "tsx:cli:migration:run": "npm run tsx:cli migration:run",
    "tsx:cli:migration:reset": "npm run tsx:cli schema:drop && npm run migration:run",
    "migration:run": "npm run typeorm -- -d ./src/data-source.ts migration:run",
    "migration:reset": "npm run typeorm -- -d ./src/data-source.ts schema:drop && npm run migration:run",
    "migration:generate": "npm run typeorm -- -d ./src/data-source.ts migration:generate",
    "migration:revert": "npm run typeorm -- -d ./src/data-source.ts migration:revert",
    "test:with-experimental-modules": "cross-env NODE_ENV=test node --experimental-vm-modules node_modules/.bin/jest --testTimeout=10000",
    "test": "NODE_ENV=test npx jest --testTimeout=10000 --detectOpenHandles",
    "pretest": "cross-env NODE_ENV=test npm run migration:reset",
    "seed": " ts-node ./src/databases/seeds/seed.ts",
    "create:seed": "ts-node ./node_modules/typeorm-extension/bin/cli.cjs seed:create  --name src/databases/seeds/user.ts"
  },
  "devDependencies": {
    "@babel/core": "^7.26.7",
    "@babel/preset-env": "^7.26.7",
    "@babel/preset-typescript": "^7.26.0",
    "@types/bcrypt": "^5.0.1",
    "@types/express": "^4.17.20",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/memory-cache": "^0.2.4",
    "@types/node": "^22.12.0",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.17",
    "@types/supertest": "^6.0.2",
    "babel-jest": "^29.7.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "socket.io-client": "^4.8.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.20",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@sendgrid/mail": "^8.1.4",
    "@types/cors": "^2.8.15",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.40.0",
    "class-validator": "^0.14.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "faker": "^5.5.3",
    "ioredis": "^5.4.2",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "memory-cache": "^0.2.0",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.10.0",
    "nodemon": "^3.1.9",
    "pg": "^8.4.0",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^4.8.1",
    "tsx": "^4.19.2",
    "typeorm-extension": "^3.6.3"
  }
}
